<h1>Listing site_data</h1>

<table>
  <thead>
    <tr>
      <th>Inventory</th>
      <th>Pappy</th>
      <th>Pappytype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @site_data.each do |site_datum| %>
      <tr>
        <td><%= site_datum.inventory %></td>
        <td><%= site_datum.pappy %></td>
        <td><%= site_datum.pappyType %></td>
        <td><%= link_to 'Show', site_datum %></td>
        <td><%= link_to 'Edit', edit_site_datum_path(site_datum) %></td>
        <td><%= link_to 'Destroy', site_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Site datum', new_site_datum_path %>
